{
  "name": "cypress-test-tiny",
  "version": "1.0.0",
  "description": "Small project for quickly testing Cypress.io test runner",
  "scripts": {
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cypress:verify": "cypress verify",
    "cypress:version": "cypress version",
    "cypress:info": "cypress info",
    "cypress:cache:list": "cypress cache list",
    "list-videos": "ls -l cypress/videos",
    "check-videos": "ok-file 'cypress/videos/*.mp4'",
    "set-status": "set-status",
    "with-faker": "cypress run --spec '**/*with**'",
    "without-faker": "cypress run --spec '**/*no**'",
    "with-faker-esbuild": "cypress run --spec '**/*with**' --config-file cypress-esbuild.config.js",
    "without-faker-esbuild": "cypress run --spec '**/*no**' --config-file cypress-esbuild.config.js"
  },
  "keywords": [],
  "author": "Gleb Bahmutov <gleb@cypress.io>",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "ok-file": "1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress-test-tiny.git"
  },
  "dependencies": {
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^12.7.0"
  }
}
